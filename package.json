{
  "name": "news-covid19",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "export NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service serve",
    "build": "export NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service build",
    "test": "export NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service test:unit a -u",
    "lint": "export NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service lint",
    "build:prod": "export NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service build --mode production",
    "build:staging": "export NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service build --mode staging"
  },
  "dependencies": {
    "apexcharts": "^3.35.0",
    "axios": "^0.26.0",
    "core-js": "^3.19.0",
    "dotenv-webpack": "^1.8.0",
    "es6-promise": "^4.2.8",
    "eslint-config-airbnb-base": "^14.0.0",
    "leaflet": "^1.6.0",
    "leaflet-search": "^2.9.11",
    "leaflet.markercluster": "^1.5.3",
    "node-forge": "^1.3.0",
    "serialize-javascript": "^5.0.1",
    "vue": "^2.6.13",
    "vue-analytics": "^5.22.1",
    "vue-apexcharts": "^1.6.2",
    "vue2-leaflet": "^2.7.0",
    "vue2-leaflet-markercluster": "^3.1.0",
    "vuetify": "^2.5.10",
    "vuex": "^3.6.2",
    "yarn": "^1.22.11"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.13.0",
    "@mdi/font": "^5.1.45",
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-service": "~4.3.0",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/test-utils": "^1.1.3",
    "babel-eslint": "^10.1.0",
    "deepmerge": "^4.2.2",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "fibers": "^5.0.0",
    "jest-canvas-mock": "^2.3.1",
    "jest-transform-stub": "^2.0.0",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
